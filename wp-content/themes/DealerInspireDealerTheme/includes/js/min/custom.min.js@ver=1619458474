jQuery(document).ready(function(i){isSmallMobile={any:function(){return i(window).width()<767}},767<i(window).width()&&setTimeout(function(){i("#homepage-advanced-search select").selectpicker({dropupAuto:!1}),i("body").live("search_filters_rendered",function(){i("#homepage-advanced-search select").selectpicker("refresh")}),i("#homepage-advanced-search .filters").fadeIn()},600);i(".ux-loading-item");i(window).bind("load",function(){var e=i(".ux-loading-item");e&&e.each(function(e){var t=i(this);setTimeout(function(){t.addClass("active")},400*e)})}),jQuery("#mobileHeaderDirections").attr("target","_self").attr("href","/contact-us/");var a=new Swiper(".modelRow .swiper-container",{speed:300,slidesPerGroup:4,slidesPerView:4,spaceBetween:55,watchOverflow:!0,lazy:{enabled:!0},navigation:{nextEl:".modelRow .swiper-button-next.model",prevEl:".modelRow .swiper-button-prev.model"},breakpoints:{1200:{slidesPerGroup:3,slidesPerView:3},1024:{slidesPerGroup:2,slidesPerView:2},767:{slidesPerGroup:1,slidesPerView:1}}});i('a[data-toggle="tab"]').on("shown.bs.tab",function(e){var t=i(this).parent().index();if(a&&a[t]&&(a[t].update(),a[t].navigation.update()),"function"==typeof LazyLoad)new LazyLoad}),i(window).width()<=1024&&jQuery("#small-selects #modelTab-contents li a").click(function(){var e=jQuery(this).data("title");jQuery("#modelTab .swap").html(e)}),setTimeout(function(){i(".modelRow .vehicle-box__image img").each(function(){i(this).attr("src",i(this).attr("data-original"))})},3e3)});