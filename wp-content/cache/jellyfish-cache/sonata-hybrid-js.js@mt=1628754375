function createNewEvent(e){var n;return"function"==typeof Event?n=new Event(e):(n=document.createEvent("Event")).initEvent(e,!0,!0),n}jQuery(document).ready(function() { var jfEventName = 'jellyfishLoaded'; jQuery(document).trigger(jfEventName); var __event = createNewEvent(jfEventName); document.dispatchEvent(__event); }); 
